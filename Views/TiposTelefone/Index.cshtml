@model IEnumerable<GrudColorado.Models.TipoTelefone>

@{
    ViewBag.Title = "Tipos de Telefone";
}

<h1>Tipos de Telefone</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Criar Novo Tipo de Telefone</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.DescricaoTipoTelefone)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.DescricaoTipoTelefone)</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.CodigoTipoTelefone" class="btn btn-warning">Editar</a>
                    <a asp-action="Details" asp-route-id="@item.CodigoTipoTelefone" class="btn btn-info">Detalhes</a>
                    <a asp-action="Delete" asp-route-id="@item.CodigoTipoTelefone" class="btn btn-danger">Excluir</a>
                </td>
            </tr>
        }
    </tbody>
</table>

